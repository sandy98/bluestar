{
  "name": "bluestar",
  "version": "0.1.1",
  "description": "Bluestar-like utility to convert images ",
  "main": "bluestar.js",
  "bin": {"bluestar": "./bin/bluestar"},
  "scripts": {
    "test": "cd test && node ../bluestar.js kw.png && cd ..",
    "build": "cp bluestar.js bin/bluestar && cd bin && chmod +x bluestar && pkg ../bluestar.js && cd ..",
    "clean": "rm bin/* && rm test/*.b64 && rm test/*.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandy98/bluestar.git"
  },
  "keywords": [
    "images",
    "bluestar",
    "embedded"
  ],
  "author": "Domingo E. Savoretti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sandy98/bluestar/issues"
  },
  "homepage": "https://github.com/sandy98/bluestar#readme",
  "dependencies": {
    "pkg": "^4.3.0-beta.1"
  }
}
